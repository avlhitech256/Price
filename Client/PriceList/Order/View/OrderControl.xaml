<searchControl:SearchControl x:Class="Order.View.OrderControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Order.View"
             xmlns:searchControl="clr-namespace:CommonControl.SearchControl;assembly=CommonControl"
             xmlns:search="clr-namespace:Order.View.Search"
             xmlns:resultSearch="clr-namespace:Order.View.ResultSearch"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="1100">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <search:MainSearchControl Grid.Row="0"/>
        <resultSearch:ResultSearchGridControl Grid.Row="1"/>
    </Grid>
</searchControl:SearchControl>
